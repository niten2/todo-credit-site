{
  "name": "credit-site",
  "version": "0.1.0",
  "description": "description",
  "author": "",
  "license": "*",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "dev": "npm run build && npm run watch-debug",
    "serve": "NODE_PATH=dist/ nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "tests": "jest --forceExit",
    "test": "jest",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "NODE_PATH=dist/ nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": ["ts", "js"],
    "moduleDirectories": ["node_modules", "spec", "src"],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/spec/**/*.spec.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "fbgraph": "^1.3.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.0-rc0",
    "mongoose": "^4.13.7",
    "morgan": "^1.7.0",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-google-token": "^0.1.2",
    "passport-local": "^1.0.0",
    "request": "^2.78.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.33",
    "@types/dotenv": "^2.0.23",
    "@types/express": "^4.11.0",
    "@types/express-session": "0.0.32",
    "@types/jest": "^19.2.2",
    "@types/jquery": "^2.0.48",
    "@types/lodash": "^4.14.91",
    "@types/mongodb": "^2.2.18",
    "@types/mongoose": "^4.7.30",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.51",
    "@types/passport": "^0.3.5",
    "@types/passport-facebook": "^2.1.7",
    "@types/request": "0.0.45",
    "@types/supertest": "^2.0.4",
    "concurrently": "^3.5.1",
    "esnext": "^3.2.1",
    "fork-ts-checker-webpack-plugin": "^0.2.9",
    "jest": "^19.0.2",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.3",
    "shelljs": "^0.7.8",
    "supertest": "^2.0.1",
    "ts-jest": "^19.0.8",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.0-dev.20171223",
    "webpack": "^3.10.0"
  }
}
