{
  "name": "credit-site",
  "version": "0.1.0",
  "description": "description",
  "author": "",
  "license": "*",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run lint",
    "dev": "npm run build && npm run watch-debug",
    "serve": "NODE_PATH=dist/ nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "tests": "jest --forceExit",
    "test": "jest",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "NODE_PATH=dist/ nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "spec",
      "src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/spec/**/*.spec.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "compression": "^1.7.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "factory-girl": "^5.0.2",
    "faker": "^4.1.0",
    "fbgraph": "^1.4.1",
    "graphql": "^0.12.3",
    "graphql-tools": "^2.16.0",
    "intel": "^1.2.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^3.0.1",
    "mongoose": "^5.0.0-rc2",
    "morgan": "^1.9.0",
    "passport": "0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-token": "^0.1.2",
    "passport-local": "^1.0.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.46",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.8",
    "@types/compression": "0.0.35",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.0",
    "@types/express-session": "1.15.6",
    "@types/faker": "^4.1.2",
    "@types/graphql": "^0.11.7",
    "@types/jest": "^22.0.1",
    "@types/jquery": "^3.2.17",
    "@types/lodash": "^4.14.92",
    "@types/mongodb": "^2.2.18",
    "@types/mongoose": "^4.7.32",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.51",
    "@types/passport": "^0.4.2",
    "@types/passport-facebook": "^2.1.7",
    "@types/request": "2.0.9",
    "@types/supertest": "^2.0.4",
    "concurrently": "^3.5.1",
    "esnext": "^3.2.1",
    "fork-ts-checker-webpack-plugin": "^0.2.10",
    "jest": "^22.0.4",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.7",
    "shelljs": "^0.7.8",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.0-dev.20171223",
    "webpack": "^3.10.0"
  }
}
